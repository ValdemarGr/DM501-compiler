
class B {
    val b = 9;
};

class A {
    val first = 5;
    var internalB: class B;
    var ifield: int;
    val printer = (): void -> {
        write ifield;
    };
};

var a: class A;
var clsB: class B;
allocate a;
allocate clsB;
allocate a.internalB;


write a.first;
write a.internalB.b;
write a.internalB.b;
write clsB.b;


allocate a;
allocate a.internalB;

write a.first;
write a.internalB.b;
write clsB.b;

a.ifield = 42;

a.printer();